{
    "manifest_version": 2,
    "name": "Ibai te Ayuda a Estudiar",
    "version": "0.1",
    "description": "Build an Extension!",
    "permissions": ["storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
        "default_icon": "images/ibai.png",
        "default_popup": "popup.html"
    }
  }